jcd() {
  jc() {
    if [ -z $1 ]; then
      exit 0
    fi
    a=${1:0:1}
    a=${a}0-${a}9

    if [ ! -z $3 ]; then
      cd /data/${a}*/${1}*/${1}.${2}*/${1}.${2}_${3}*
    elif [ ! -z $2 ]; then
      cd /data/${a}*/${1}*/${1}.${2}*
    else
      cd /data/${a}*/${1}*
    fi
  }

  case "${#1}" in
    *2) jc ${1} ;;
    *4) jc ${1:0:2} ${1:2} ;;
    *5) jc ${1:0:2} ${1:3} ;;
    *6) jc ${1:0:2} ${1:2:2} ${1:4:2} ;;
    *8) jc ${1:0:2} ${1:3:2} ${1:6:2} ;;
  esac
}
export jcd

